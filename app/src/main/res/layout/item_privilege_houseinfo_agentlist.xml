<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <com.flzc.fanglian.view.CircleImg
        android:id="@+id/img_item_privilege_houseInfo_agentList"
        android:layout_width="46dip"
        android:layout_height="46dip"
        android:layout_marginBottom="14dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="14dip"
        android:src="@drawable/loading_90_90" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_name_item_privilege_houseInfo_agentList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:text="房链经纪人326"
            android:textColor="#333333"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_peopleCount_item_privilege_houseInfo_agentList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:text="累计带看：3人"
            android:textColor="#666666" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/img_phone_item_privilege_houseInfo_agentList"
            android:layout_width="33dip"
            android:layout_height="33dip"
            android:src="@drawable/phone_agent_list" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_gravity="center_horizontal"
            android:text="咨询"
            android:textColor="#333333"
            android:textSize="12sp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dip"
        android:layout_marginLeft="30dp"
        android:layout_gravity="center_vertical"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/img_time_ordered"
            android:layout_width="33dip"
            android:layout_height="33dip"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/phone_agent_time" />

        <TextView
            android:id="@+id/tv_yuyue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:layout_gravity="center_horizontal"
            android:text="预约"
            android:textColor="#333333"
            android:textSize="12sp" />
    </LinearLayout>

   

</LinearLayout>